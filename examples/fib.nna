; Bringing you that hot, hot
; and probably broken assembly!
;
; It's gonna be Fibonacci
; because that's cool
;
; ra --> iterator
; rb --> fib i
; rc --> fib i+1
; rd --> temp

VAL num 10000000

LBL fib
MOV 0 $r1
MOV 0 $r2
MOV 1 $r3
LBL while_cond
	CLT $r1 <num
	JCH while_body
	HLT

LBL while_body
	MOV $r3 $r4
	ADD $r2 $r3
	MOV $r4 $r2
	ADD 1 $r1
	JMP while_cond
