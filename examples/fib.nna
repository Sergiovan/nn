; and probably broken assembly!
;
; It's gonna be Fibonacci
; because that's cool
;
; ra --> iterator
; rb --> fib i
; rc --> fib i+1
; rd --> temp

VAL num 100000000

LBL fib
MOV 0 $ra
MOV 0 $rb
MOV 1 $rc
LBL while_cond
	CLT $ra <num
	JCH while_body
	HLT

LBL while_body
	MOV $rc $rd
	ADD $rb $rc
	MOV $rd $rb
	ADD 1 $ra
	JMP while_cond
